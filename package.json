{
  "name": "@rangermauve/web-loader",
  "version": "1.0.0",
  "description": "A custom module loader and global shim for Node to make it compatible with the browser.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "node": "node",
    "example": "node --experimental-modules --loader ./loader.mjs example.js",
    "example-dat": "node --experimental-modules --loader ./loader.mjs example-dat.js",
    "example-download": "node --experimental-modules ./httpload.mjs https://rangermauve.hashbase.io/esm.js",
    "example-dat-download": "node --experimental-modules ./datload.mjs dat://rangermauve.hashbase.io/esm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RangerMauve/node-web-loader.git"
  },
  "keywords": [
    "esm",
    "ecmascript",
    "modules",
    "loader"
  ],
  "author": "rangermauve",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RangerMauve/node-web-loader/issues"
  },
  "homepage": "https://github.com/RangerMauve/node-web-loader#readme",
  "devDependencies": {
    "node": "^10.8.0"
  },
  "dependencies": {
    "fake-indexeddb": "^2.0.4",
    "form-data": "^2.3.2",
    "mkdirp": "^0.5.1",
    "node-dat-archive": "^2.0.1",
    "node-fetch": "^2.2.0",
    "node-localstorage": "^1.3.1",
    "ws": "^6.0.0"
  }
